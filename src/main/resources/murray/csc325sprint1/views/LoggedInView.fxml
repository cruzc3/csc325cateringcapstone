<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Logged In Successfully!" style="-fx-font-size: 24px; -fx-font-weight: bold;"/>
    <Label text="Welcome to the Catering App" style="-fx-font-size: 18px;"/>

    <Button text="Continue to Main Menu" onAction="#handleContinue" style="-fx-font-size: 14px;"/>
</VBox>